{
  "private": true,
  "repository": "https://github.com/nickschot/ember-gesture-modifiers",
  "license": "MIT",
  "author": "Nick Schot <nick@sophorasoft.nl>",
  "scripts": {
    "build": "pnpm --filter ember-gesture-modifiers build",
    "changelog": "lerna-changelog --from=v0.1.0 > CHANGELOG.md",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-gesture-modifiers start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "lerna-changelog": "2.2.0",
    "prettier": "3.2.2",
    "prettier-plugin-ember-template-tag": "1.1.0"
  },
  "pnpm": {
    "overrides": {
      "@types/eslint": "^7.0.0"
    }
  },
  "volta": {
    "node": "18.19.0",
    "pnpm": "8.14.1"
  },
  "engines": {
    "node": ">= 18"
  }
}
